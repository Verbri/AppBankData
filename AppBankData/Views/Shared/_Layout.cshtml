<!DOCTYPE html>
<html>
<head>
    <meta name="keywords" content="" />
    <meta name="description" content="" />
    <meta name="robots" content="index, follow" />
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=0.45">
    <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description" />
    <meta Http-Equiv="Cache-Control" Content="no-cache" />
    <meta Http-Equiv="Pragma" Content="no-cache" />
    <meta Http-Equiv="Expires" Content="0" />
    <meta content="Coderthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Content/fontawesome-free/css/all.min.css">
    <!-- daterange picker -->
    <link href="~/Content/daterangepicker/daterangepicker.css" rel="stylesheet" />
    <!-- Tempusdominus Bootstrap 4 for datetimepicker-->
    <link href="~/Content/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css" rel="stylesheet" />
    <!-- DataTables -->
    <link href="~/Content/datatables-bs4/css/dataTables.bootstrap4.css" rel="stylesheet" />

    <link rel="stylesheet" href="~/Content/datatables-responsive/css/responsive.bootstrap4.min.css">
    <link rel="stylesheet" href="~/Content/datatables-buttons/css/buttons.bootstrap4.min.css">

    <!-- Select2 -->
    <link href="~/Content/select2/css/select2.min.css" rel="stylesheet" />
    <link href="~/Content/select2-bootstrap4-theme/select2-bootstrap4.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/dist/css/adminlte.css">
    <link rel="stylesheet" href="~/Content/Style.css">
    <!-- SweetAlert2 -->
    <link href="~/Content/sweetalert2-theme-bootstrap-4/bootstrap-4.min.css" rel="stylesheet" />
    <!-- Toastr -->
    <link href="~/Content/toastr/toastr.min.css" rel="stylesheet" />
    @*Telerik*@
    <link href="~/Content/ReportViewer/styles/kendo.common.min.css" rel="stylesheet" />
    <link href="~/Content/ReportViewer/styles/kendo.blueopal.min.css" rel="stylesheet" />



</head>
<body class="hold-transition sidebar-mini layout-fixed layout-navbar-fixed layout-footer-fixed">

    <div class="wrapper">
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-dark">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
            </ul>
            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">
                <!-- Notifications Dropdown Menu -->
                <li class="nav-item">
                    <a class="nav-link" data-widget="fullscreen" href="#" role="button">
                        <i class="fas fa-expand-arrows-alt"></i>
                    </a>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link" data-toggle="dropdown" href="#">
                        <i class="fas fa-cog"></i>
                    </a>
                    <div class="dropdown-menu dropdown-menu-md dropdown-menu-right">
                        <a href="#" class="dropdown-item">
                            <i class="fas fa-solid fa-key mr-2"></i>Ganti Password
                        </a>
                        <div class="dropdown-divider"></div>
                        <a href="~/Login/Logout" class="dropdown-item">
                            <i class="fas fa-sign-out-alt"></i> Keluar
                        </a>
                    </div>
                </li>
            </ul>
        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="@Url.Action("Index", "Home")" class="brand-link">
                <img src="~/img/imgrs.png" alt="Immanuel Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
                <span class="brand-text font-weight-bold">BANK DATA</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="~/img/Default.jpg" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a class="brand-text font-weight-bold text-white" style="font-size: 13px;">@Session["Nama"]</a>
                        <a href="#" class="d-block">@Session["Bagian"]</a>
                    </div>
                </div>

                <!-- SidebarSearch Form -->
                <div class="form-inline">
                    <div class="input-group" data-widget="sidebar-search">
                        <input class="form-control form-control-sidebar" type="search" placeholder="Search" aria-label="Search">
                        <div class="input-group-append">
                            <button class="btn btn-sidebar">
                                <i class="fas fa-search fa-fw"></i>
                            </button>
                        </div>
                    </div>
                </div>
                @{
                    // Definisikan array activeMenus untuk memeriksa menu yang aktif
                    var activeListKomputer = new List<string> { "ListKomputer", "Komputer", "" };

                    // Cek apakah ViewBag.ActiveMenu ada dalam array activeMenus
                    var isActiveKomputer = activeListKomputer.Contains(ViewBag.Menu) ? "active" : "";
                    var isMenuKomputer = activeListKomputer.Contains(ViewBag.Menu) ? "menu-open" : "";
                }


                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class with font-awesome or any other icon font library -->
                        <li class="nav-item @isMenuKomputer">
                            <a href="javascript: void(0);" class="nav-link @isActiveKomputer">
                                <i class="nav-icon fas fa-chart-pie"></i>
                                <p>
                                    KOMPUTER
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("Index", "ListKomputer")" class="nav-link @(ViewBag.Menu == "ListKomputer" ? "active" : "")">
                                        &emsp;<i class="far fa-circle nav-icon"></i>
                                        <p>LIST KOMPUTER </p>
                                    </a>
                                </li>

                                <li class="nav-item">
                                    <a href="@Url.Action("Index", "Home")" class="nav-link @(ViewBag.Active == "Home" ? "active" : "")">
                                        &emsp;<i class="far fa-circle nav-icon"></i>
                                        <p>
                                            LIST KOMPUTER
                                        </p>

                                    </a>
                                </li>

                            </ul>
                        </li>
                        @{
                            // Definisikan array activeMenus untuk memeriksa menu yang aktif
                            var activeSetting = new List<string> { "Apps&Program", "User", "" };

                            // Cek apakah ViewBag.ActiveMenu ada dalam array activeMenus
                            var isActiveSetting = activeSetting.Contains(ViewBag.Menu) ? "active" : "";
                            var isMenuSetting = activeSetting.Contains(ViewBag.Menu) ? "menu-open" : "";
                        }
                        <li class="nav-item @isMenuSetting">
                            <a href="javascript: void(0);" class="nav-link @isActiveSetting">
                                <i class="nav-icon fas fa-cogs"></i>
                                <p>
                                    SETTING
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("Index", "InstalledAppsProgram")" class="nav-link @(ViewBag.Menu == "Apps&Program" ? "active" : "")">
                                        &emsp;<i class="far fa-circle nav-icon"></i>
                                        <p>PARAMETER APPS</p>
                                    </a>
                                </li>

                                <li class="nav-item">
                                    <a href="@Url.Action("Index", "User")" class="nav-link @(ViewBag.Menu == "User" ? "active" : "")">
                                        &emsp;<i class="far fa-circle nav-icon"></i>
                                        <p>
                                            USER
                                        </p>
                                    </a>
                                </li>

                            </ul>
                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper pt-2 pb-4">
            <!-- Main content -->
            <section class="content">
                <div class="container-fluid">
                    <div id="loadingSpinner">
                        <img src="~/img/12 segments.gif" alt="Loading..." /> <!-- Gambar spinner -->
                    </div>
                    @RenderBody()
                </div>
                <!-- /.container-fluid -->
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer p-1">
            <div class="float-right d-none d-sm-inline-block">
                <b>Version</b>1.1.0  @ViewBag.Database
            </div>
            <strong>&copy; 2019-2024</strong> RS. IMMANUEL

        </footer>


    </div>
    <!-- jQuery -->
    <script src="~/Content/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/Content/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Select2 -->
    <script src="~/Content/select2/js/select2.full.min.js"></script>
    <!-- InputMask -->
    <script src="~/Content/moment/moment.min.js"></script>
    <!-- DataTables  & Plugins -->
    <script src="~/Content/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Content/datatables-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/Content/datatables-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/Content/datatables-responsive/js/responsive.bootstrap4.min.js"></script>
    <script src="~/Content/datatables-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/Content/datatables-buttons/js/buttons.bootstrap4.min.js"></script>
    <script src="~/Content/jszip/jszip.min.js"></script>
    <script src="~/Content/pdfmake/pdfmake.min.js"></script>
    <script src="~/Content/pdfmake/vfs_fonts.js"></script>
    <script src="~/Content/datatables-buttons/js/buttons.html5.min.js"></script>
    <script src="~/Content/datatables-buttons/js/buttons.print.min.js"></script>
    <script src="~/Content/datatables-buttons/js/buttons.colVis.min.js"></script>
    <!-- date-range-picker -->
    <script src="~/Content/daterangepicker/daterangepicker.js"></script>
    <!-- Tempusdominus Bootstrap 4 for datetime picker -->
    <script src="~/Content/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>

    @*Plugins Chart dan label chart*@
    <script src="~/Content/chart.js/Chart.min.js"></script>
    <script src="~/Content/chart.js/chartjs-plugin-datalabels.min.js"></script>

    <!-- AdminLTE App -->
    <script src="~/Content/dist/js/adminlte.min.js"></script>
    <!-- SweetAlert2 -->
    <script src="~/Content/sweetalert2/sweetalert2.min.js"></script>
    <!-- Toastr -->
    <script src="~/Content/toastr/toastr.min.js"></script>
    <!--Telerik-->
    <script src="~/Content/ReportViewer/js/telerikReportViewer-17.0.23.118.min.js"></script>
    <script>
    $(document).ajaxError(function (event, xhr, settings) {
        if (xhr.status === 401) {
            Swal.fire({
                title: "Sesi Habis",
                text: "Sesi login Anda sudah berakhir. Silakan login ulang.",
                icon: "warning",
                confirmButtonText: "Login Ulang",
                allowOutsideClick: false,
            }).then(function () {
                window.location.href = '@Url.Action("Login", "Account")';
            });
        }
    });
    </script>

    @RenderSection("scripts", required: false)


</body>
</html>
